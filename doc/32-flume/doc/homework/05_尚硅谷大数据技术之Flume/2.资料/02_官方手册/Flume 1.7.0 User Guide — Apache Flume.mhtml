<span class=3D"na">a1.sources.r1.channels</span> <span class=3D"o">=3D</spa=
n> <span class=3D"s">c1</span>
<span class=3D"na">a1.sources.r1.initialContextFactory</span> <span class=
=3D"o">=3D</span> <span class=3D"s">org.apache.activemq.jndi.ActiveMQInitia=
lContextFactory</span>
<span class=3D"na">a1.sources.r1.connectionFactory</span> <span class=3D"o"=
>=3D</span> <span class=3D"s">GenericConnectionFactory</span>
<span class=3D"na">a1.sources.r1.providerURL</span> <span class=3D"o">=3D</=
span> <span class=3D"s">tcp://mqserver:61616</span>
<span class=3D"na">a1.sources.r1.destinationName</span> <span class=3D"o">=
